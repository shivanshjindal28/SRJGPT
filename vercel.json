{
    "version": 2,
    "functions": {
        "api/*.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        { "src": "/", "dest": "run.py" },
        { "src": "/api/(.*)", "dest": "api/$1.py" },
        { "src": "/(.*)", "dest": "run.py" }
    ]
} 